<div id="div_hide">
  <% if @surveys != [] %>
    <ul>
      <% @surveys.each do |s| %>
        <li>
          <a href="/survey/<%= s.id %>">
            <%= s.name %>
          </a>
        </li>
      <% end %>
    </ul>
  <% else %>
  <p><strong>No hay encuestas de otros usuarios :(</strong></p>
  <% end %>  
</div>  
